# Backend Services
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o
ASSEMBLYAI_API_KEY=your_assemblyai_api_key

# Database & Storage
DATABASE_URL=postgresql://user:password@host:5432/database?sslmode=require
OBJECT_STORAGE_BUCKET=patient-files
OBJECT_STORAGE_ENDPOINT=https://your-supabase-project.supabase.co/storage/v1
OBJECT_STORAGE_ACCESS_KEY=your_supabase_anon_key
OBJECT_STORAGE_SECRET_KEY=your_supabase_service_role_key

# Security
JWT_SIGNING_KEY=your_jwt_signing_key
ENCRYPTION_KEY=your_encryption_key

# Server Configuration
PORT=3000
NODE_ENV=development
FRONTEND_URL=http://localhost:3001
BACKEND_URL=http://localhost:3000

# Google Calendar OAuth
GOOGLE_CALENDAR_OAUTH_CLIENT_ID=your_google_client_id
GOOGLE_CALENDAR_OAUTH_CLIENT_SECRET=your_google_client_secret

# Microsoft Graph OAuth (for future use)
MS_GRAPH_CLIENT_ID=your_ms_client_id
MS_GRAPH_CLIENT_SECRET=your_ms_client_secret

# Email Service (Resend)
RESEND_API_KEY=your_resend_api_key
